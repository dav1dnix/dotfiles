{
    "workbench.startupEditor": "none",
    "emojisense.unicodeCompletionsEnabled": true, // enable completions that insert emoji as unicode
    "emojisense.markupCompletionsEnabled": true, // enable completions that insert emoji markup
    "emojisense.showOnColon": true, // show emoji when typing colon
    "emojisense.emojiDecoratorsEnabled": true, // show emoji decorators inline

    // Emojisense languages
    "emojisense.languages": {
        "html": true,
        "javascript": true,
        "python": true,
        "css": true,
        "sql": true,
        "typescript": true,
    },
    "discord.detailsEditing": "Editing {filename} (Line {currentline}/{totallines})",
    "terminal.integrated.shell.linux": "/bin/zsh",
    "sshfs.configs": []
}
