! XTerm
xterm*termName: xterm-256color
xterm*faceName: cozette
xterm*faceSize: 11
xterm*reverseVideo: false

xterm*cursorColor: #fff

! Border
xterm*internalBorder: 24

! Translations
xterm*VT100.Translations: #override \
  Meta <Key> minus: smaller-vt-font() \n\
  Ctrl Shift <Key>V: insert-selection(CLIPBOARD) \n\
  Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
  Meta <Key> plus: larger-vt-font() \n

!URxvt
URxvt.scrollBar: false
URxvt.reverseVideo: true
URxvt*scrollTtyKeypress: true
URxvt.font: xft:Cozette
URxvt.letterSpace: -1
URxvt*scrollTtyOutput: false
! special
*.foreground:   #b9b9b9
*.background:   #101010
*.cursorColor:  #b9b9b9

! black
*.color0:       #101010
*.color8:       #525252

! red
*.color1:       #7c7c7c
*.color9:       #7c7c7c

! green
*.color2:       #8e8e8e
*.color10:      #8e8e8e

! yellow
*.color3:       #a0a0a0
*.color11:      #a0a0a0

! blue
*.color4:       #686868
*.color12:      #686868

! magenta
*.color5:       #747474
*.color13:      #747474

! cyan
